html, body
  font-family "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro0"

li
  list-style: none

#wrapper
  padding: 0 20px

.navbar
  height 4em
  padding 5px 30px
  box-shadow 1px 0px 1px 1px black


.logo
  float: left
  height: 100%
  img
    height: 100%

.menu
  display flex
  float: right
  &__item
    margin-left: 10px

.container
  h1
    text-align center
  img
    width 100%
  &-rows
    > *
      width 50%
